
printLine s return:
    print s (:)
    print "\n" (:)
    return

askNum question return:
    print question (:)
    return (parseInt (input))




fact n return:
    isZero n (:return 1) (:)
    return (mul n (fact (sub n 1)))

fib n return:
    isZero n (:return 0) (:sub n 1 (m:))
    isZero m (:return 1) (:sub m 1 (k:))
    return (add (fib m) (fib k))

main return:
    printLine "Hello, World!" (:)
    askNum "Please enter a number: " (n:)
    print n (:)
    print " factorial is " (:)
    printLine (fact n) (:)
    print n (:)
    print " fibonaci is " (:)
    printLine (fib n) (:)
    return

main exit
